<Window x:Class="AngelMacro.CompilingCodeMessageBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:rs="clr-namespace:AngelMacro.Properties"
        xmlns:local="clr-namespace:AngelMacro"
        mc:Ignorable="d"
        Title="{x:Static rs:Resources.compiling_code}" Height="64" Width="256" FontFamily="Consolas" ResizeMode="NoResize" WindowStartupLocation="CenterOwner">
    <Grid>
        <ProgressBar x:Name="CompilingProgress" Margin="5"/>
    </Grid>
</Window>
